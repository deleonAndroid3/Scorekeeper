<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimary" />
            <padding
                android:bottom="12dp"
                android:left="12dp"
                android:right="12dp"
                android:top="12dp" />
            <size
                android:height="70dp"
                android:width="70dp"/>

        </shape>
    </item>
</selector>